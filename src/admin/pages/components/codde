
<template>
  <div class="table-container">
    <table class="table">
      <thead>
        <tr>
          <th>Colonne 1</th>
          <th>Colonne 2</th>
        </tr>
      </thead>
      <tbody>
        <!-- Utilisation de v-for pour parcourir les éléments du tableau -->
        <tr v-for="(item, index) in items" :key="index">
          <td>{{ item.column1 }}</td>
          <td>{{ item.column2 }}</td>
        </tr>
      </tbody>
    </table>
  </div>
</template>

<script>
export default {
  data() {
    return {
      // Exemple de données pour le tableau
      items: [
        { column1: 'Donnée 1A', column2: 'Donnée 1B' },
        { column1: 'Donnée 2A', column2: 'Donnée 2B' },
        { column1: 'Donnée 3A', column2: 'Donnée 3B' }
        // Ajoutez autant d'éléments que nécessaire
      ]
    };
  }
};
</script>

<style scoped>
.table-container {
  max-width: 600px;
  margin: 0 auto;
}

.table {
  width: 100%;
  border-collapse: collapse;
}

.table th,
.table td {
  border: 1px solid #ddd;
  padding: 8px;
  text-align: left;
}

.table th {
  background-color: #f2f2f2;
}
</style>








